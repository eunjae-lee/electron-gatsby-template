{
  "name": "electron-gatsby-template",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Electron + Gatsby",
  "version": "0.1.0",
  "author": "Eunjae Lee <karis612@gmail.com>",
  "main": "src/electron.js",
  "scripts": {
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "develop": "concurrently \"cross-env BROWSER=none yarn react-develop\" \"wait-on http://localhost:3456 && electron .\"",
    "react-build": "gatsby build",
    "react-develop": "gatsby develop --port 3456",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "react-test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@types/react-helmet": "^5.0.15",
    "cross-env": "^7.0.0",
    "electron-context-menu": "^0.16.0",
    "electron-debug": "^3.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-unhandled": "^3.0.2",
    "electron-util": "^0.13.1",
    "gatsby": "^2.19.7",
    "gatsby-plugin-typescript": "^2.1.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-react-devtools": "^0.5.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "wait-on": "^4.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eunjae-lee/electron-gatsby-template"
  },
  "bugs": {
    "url": "https://github.com/eunjae-lee/electron-gatsby-template/issues"
  },
  "build": {
    "appId": "com.company.AppName",
    "mac": {
      "category": "public.app-category.social-networking",
      "darkModeSupport": true
    },
    "dmg": {
      "iconSize": 160,
      "contents": [
        {
          "x": 180,
          "y": 170
        },
        {
          "x": 480,
          "y": 170,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Network;Chat"
    }
  }
}
